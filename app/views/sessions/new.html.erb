<h1>Sign In</h1>

<form class="" action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label>Username
    <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>

  <label>Password
    <input type="password" name="user[password]" value="<%= @user.password %>">
  </label>

  <input type="submit" name="" value="Sign In">
</form>
